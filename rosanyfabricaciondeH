  - ID_Area: int
  - tipo: string
  - capacidad: int
  - estado: string
  - ubicacion: string
}

Class Producto_final {
  - ID_Producto: int
  - nombre: string
  - tipo: string
  - cantidad: int
  - ubicacion: string
}

Class Moldes {
  - ID_Molde: int
  - tipo: string
  - material: string
  - estado: string
  - ubicacion: string
}

